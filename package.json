{
  "name": "node-client-bp",
  "version": "0.0.1",
  "description": "A simple client boilerplate with Node, Typescript and React",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --watch",
    "bundle:parcel": "cross-env NODE_ENV=production rm -rf dist/ && parcel build src/index.html  --public-url ./",
    "serve:dev": "cross-env NODE_ENV=development rm -rf dist/ && parcel src/index.html -p 8080"
  },
  "author": "James Abels",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
